<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hybrid.controllers.TableController">
   <left>
      <Button fx:id="btnSelect" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Table" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <TableView fx:id="dept" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="deptno" minWidth="50.0" prefWidth="-1.0" text="deptno" />
          <TableColumn fx:id="dname" minWidth="50.0" prefWidth="-1.0" text="dname" />
            <TableColumn fx:id="loc" minWidth="50.0" prefWidth="-1.0" text="loc" />
        </columns>
      </TableView>
   </center>
</BorderPane>
