<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hybrid.controllers.InjectController">
   <center>
      <ListView fx:id="listResult" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <Button fx:id="btnSelect" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="List" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
      <Button fx:id="btnIncrement" mnemonicParsing="false" text="Increment" BorderPane.alignment="CENTER">
         <graphic>
            <ProgressBar fx:id="progress" prefWidth="200.0" progress="0.0" />
         </graphic>
      </Button>
   </bottom>
</BorderPane>
